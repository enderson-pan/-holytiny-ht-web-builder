{
    "name": "htweb-builder-cli",
    "version": "0.0.2",
    "description": "A CLI tool to generate npm scripts for both front-end and back-end projects to make the build process convenient.",
    "main": "./lib/index.js",
    "repository": "https://github.com/enderson-pan/-holytiny-ht-web-builder.git",
    "author": "Sake <pan.ziqiang@holytiny.com>",
    "license": "MIT",
    "bin": {
        "htwb": "./lib/index.js"
    },
    "preferGlobal": true,
    "private": false,
    "scripts": {
        "babel": "babel ./src -d ./lib",
        "babel:watch": "babel ./src -d ./lib -w",
        "start": "node lib",
        "ht-transpiler": "Babel ./src -d ./lib ",
        "ht-transpiler:watch": "Babel ./src -d ./lib  -w"
    },
    "dependencies": {
        "babel-cli": "6.26.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-env": "1.6.1",
        "caporal": "0.9.0",
        "chalk": "2.3.0",
        "clear": "0.0.1",
        "figlet": "1.2.0",
        "inquirer": "5.1.0",
        "winston": "2.4.0"
    }
}